<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.backintime">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@drawable/tictaktoe"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:roundIcon="@drawable/tictaktoe"
        android:supportsRtl="true"
          android:theme="@style/AppTheme">
        <activity android:name=".OnlineLoginActivity"></activity>
        <activity android:name=".gamelevel" />
        <activity android:name=".Playername" />
        <activity android:name=".About" />
        <activity android:name=".Settings" />
        <activity android:name=".Online" />
        <activity android:name=".PlayWithFreinds" />
        <activity
            android:name=".PlayerMode"
            android:theme="@style/playermode" />
        <activity
            android:name=".Splash_Screen"
            android:theme="@style/OurOWn">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
        <activity android:name=".playOneonOne" />

        <service android:name=".MusicService" />
        <service android:name=".MusicServicePlaying" />
    </application>

</manifest>